{
  "name": "solid-markdown-wasm",
  "private": false,
  "version": "1.5.1",
  "type": "module",
  "module": "./dist/solid-markdown-wasm.es.js",
  "types": "./dist/solid-markdown-wasm.es.d.ts",
  "workspaces": ["example"],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
