  },
  "peerDependencies": {
    "solid-js": "^1.9.5"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "solid-js": "^1.9.5",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-solid": "^2.11.6"
  }
